@startuml

class Model {
    +setGameState(gameState: Object): void
    +getGameState(): Object
}

class View {
    +displayGameBoard(): void
    +displayGameOver(): void
    +displayGameStart(): void
}

class Controller {
    -model: Model
    -view: View
    +handleInput(input: Object): void
}

Model -> Controller
Controller -> View
View --> Model
@enduml
